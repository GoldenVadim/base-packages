NAME="fastfetch"
VERSION="2.27.1"
DEPENDENCIES="cmake"
SOURCE_URL="https://github.com/fastfetch-cli/fastfetch/archive/refs/tags/2.27.1.tar.gz"
FETCH_METHOD="wget"
BUILD="cmake -B build -S . -DCMAKE_BUILD_TYPE=Release -DCMAKE_INSTALL_PREFIX=/usr && cmake --build build --parallel $(nproc)"
INSTALL="cmake --install build"
